{
    "name": "Wide profile picture fixer for Mashov",
    "version": "1.0",
    "manifest_version": 2,
    "content_scripts": [
      {
        "matches": ["https://web.mashov.info/*"],
        "js": ["content.js"]
      }
    ]
    ,
    "browser_action": {
      "default_popup": "popup.html",
      "default_title": "wide PP fixer"
    }
  }  